<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.trabajoacd.controller.HomePageController">
   <children>
      <ComboBox fx:id="connectedUsersComboBox" layoutX="436.0" layoutY="27.0" prefWidth="150.0" promptText="User conected..." />
      <ListView fx:id="listView" layoutX="144.0" layoutY="72.0" prefHeight="282.0" prefWidth="292.0" />
      <Button fx:id="button1" layoutX="468.0" layoutY="341.0" mnemonicParsing="false" onAction="#addChat" prefHeight="24.0" prefWidth="100.0" style="-fx-background-color: #09705d; -fx-border-radius: 5px;" text="Add Room" textFill="WHITE" />
      <Button fx:id="btn_room" layoutX="468.0" layoutY="293.0" mnemonicParsing="false" onAction="#handleRoomSelection" prefHeight="24.0" prefWidth="100.0" style="-fx-background-color: #09705d; -fx-border-radius: 5px;" text="Go to Room" textFill="WHITE" />
      <Button layoutX="14.0" layoutY="27.0" mnemonicParsing="false" onAction="#login" prefHeight="24.0" prefWidth="100.0" style="-fx-background-color: #09705d; -fx-border-radius: 5px;" text="Logout" textFill="WHITE" />
   </children>
</AnchorPane>
